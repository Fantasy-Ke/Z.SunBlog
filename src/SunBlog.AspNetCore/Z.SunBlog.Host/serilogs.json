{
  "Serilog": {
    // 配置日志的最小记录等级
    "MinimumLevel": {
      // 默认记录等级
      "Default": "Information",
      // 重写各类包的日志记录等级
      "Override": {
        "Microsoft": "Information",
        "System": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Information"
      }
    },
    "Properties": {
      "ApplicationName": "Z.SunBlog"
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId",
      "ApplicationContext"
    ],
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Seq"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss:fffff} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "App_Data/Logs/log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 31,
          "outputTemplate": "[{Timestamp:HH:mm:ss:fffff} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://8.134.249.130:5341/", // 日志中心收集器的链接地址
          "restrictedToMinimumLevel": "Verbose",//日志等级
          "apiKey": "b8ueTcbAyumLam5hfuUu",//密钥key
          "queueSizeLimit": 100000,//最大删除值
          "batchPostingLimit": 100//批量最多
        }
      }
    ]
  }
}
